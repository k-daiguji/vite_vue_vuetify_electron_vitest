{
  "name": "vite_vue",
  "version": "0.0.0",
  "main": "dist/background.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npm-run-all makedir:dist filecopy:* background:build --parallel dev --race electron:start",
    "background:build": "rollup --config rollup.config.js",
    "electron:start": "set NODE_ENV=development && electron .",
    "makedir:dist": "node -e \"require('fs').mkdirSync('./dist', { recursive: true })\"",
    "filecopy:preload": "node -e \"require('fs').copyFileSync('./preload.ts', './dist/preload.ts')\"",
    "serve": "vite preview",
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "electron:serve": "vite electron:serve",
    "lint": "eslint --fix src/*.{js,ts,vue} && eslint --fix src/**/*.{js,ts,vue}",
    "format": "prettier -w src/*.{js,ts,vue} && prettier -w src/**/*.{js,ts,vue}",
    "fix": "npm run format && npm run lint"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "roboto-fontface": "*",
    "vue": "^3.2.37",
    "vuetify": "npm:@vuetify/nightly@next",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/vue": "^6.6.1",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/coverage-c8": "^0.23.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.1",
    "@vue/test-utils": "^2.0.2",
    "electron": "^20.1.4",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^9.5.1",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "vite": "^3.1.0",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vitest": "^0.23.2",
    "vue-cli-plugin-vuetify": "~2.5.8"
  }
}
